            "mainClass": "it.unich.jgmpbenchmarks.benchmarks.ContinuedFractionBenchmark",
            "projectName": "benchmarks"
        },
        {
            "type": "java",
            "name": "ContinuedFractionFloatBenchmark",
            "request": "launch",
            "mainClass": "it.unich.jgmpbenchmarks.benchmarks.ContinuedFractionFloatBenchmark",
            "projectName": "benchmarks"
        },
        {
            "type": "java",
            "name": "EulerNumberDigitsBenchmark",
            "request": "launch",
            "mainClass": "it.unich.jgmpbenchmarks.benchmarks.EulerNumberDigitsBenchmark",
            "projectName": "benchmarks"
        },
        {
            "type": "java",
            "name": "FactorialBenchmark",
            "request": "launch",
            "mainClass": "it.unich.jgmpbenchmarks.benchmarks.FactorialBenchmark",
            "projectName": "benchmarks",
            "env": {
                "LD_LIBRARY_PATH": "lib"
            }
        },
        {
            "type": "java",
            "name": "PrimeBenchmark",
            "request": "launch",
            "mainClass": "it.unich.jgmpbenchmarks.benchmarks.PrimeBenchmark",
